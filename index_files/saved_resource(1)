    function omegaConvertChineseToUnicode(str) {
      var codeStr = "";
      for (var i = 0; i < str.length; i++) {
        var _code = str.charCodeAt(i).toString(16);
        codeStr += 'u' + "0000".substr(0, 4 - _code.length) + _code
      }
      return codeStr.replace('u002d', '-');
    }
